.Type1=#T.Type1 AND tblSDAll.cYear = '+Convert(Varchar, @prYear)+' AND tblSDAll.cMonth ='+ Convert(Varchar, @intYearStartingMonth )+' ' EXEC (@QRY1) SET @intYearStartingMonth = @intYearStartingMonth + 1 SET @intRound = @intRound + 1 END DELETE FROM tblPayrollAnnualSummary INSERT INTO tblPayrollAnnualSummary SELECT EmpID,[cYear],[salID],[salDesc] ,[IOD] ,[Amount],[January],[February],[March],[April],[May],[June],[July],[August],[September],[October],[November],[December] FROM #T WHERE EmpID IN (SELECT RegID from tblTempRegID) ORDER BY EmpId,iod END " : FK_EQ(sSQL, "P", "", False, False, False)

