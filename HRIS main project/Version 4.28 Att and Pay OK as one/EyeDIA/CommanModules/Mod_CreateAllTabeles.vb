        sSQL = "CREATE PROCEDURE [dbo].[SP_ViewShortLeaveSelected] (@RegIDs NVARCHAR (3500),@st DATETIME,@ed DATETIME,@maxMin INTEGER,@minMinutesForShLv INTEGER,@shLvGraceMin INTEGER,@Year INTEGER,@Month INTEGER) AS BEGIN Declare  @Xml AS XML SET @Xml = cast(('<A>'+replace(@RegIDs,',' ,'</A><A>')+'</A>') AS XML) CREATE TABLE #ShLv (RegID NVARCHAR (6),atDate DATETIME,inTime1 DATETIME,outTime1 DATETIME,lateMins NUMERIC (18,0),EarlyMins NUMERIC (18,0),workMins NUMERIC (18,0),sInTime DATETIME,sOutTime DATETIME,allShift NVARCHAR (3),autoMin NUMERIC (18,0),BalMins NUMERIC (18,0),balQty NUMERIC (18,0)) INSERT INTO #ShLv SELECT EmpId,atDate,inTime1,outTime1,latemins,earlyMins,WorkMins,sInTime,sOutTime,allShifts,0,0,0 FROM tblEmpRegister WHERE atDate BETWEEN  @st  AND  @ed AND EmpID IN (SELECT A.value('.', 'varchar(max)') as [Column] FROM @Xml.nodes('A') AS FN(A)) ORDER BY EmpID,AtDate DECLARE @rCount AS INTEGER DECLARE @intCR AS INTEGER DECLARE @rStart AS INTEGER DECLARE @rEnd AS INTEGER SET @rCount= (SELECT @maxMin/@minMinutesForShLv) SET @intCR=1 SET @rStart=0 SET @rEnd=@minMinutesForShLv CREATE TABLE #T (id NUMERIC (18,0),rStart NUMERIC(18,0),rEnd NUMERIC (18,0)) WHILE (@rCount >=@intCR) BEGIN INSERT INTO #T SELECT @intCR,@rStart+1,@rEnd SET @intCR=@intCR+1 SET @rStart=@rStart +@minMinutesForShLv SET @rEnd=@rEnd+@minMinutesForShLv END UPDATE #ShLv SET autoMin =rEnd FROM #ShLv,#T WHERE (#ShLv.lateMins-@shLvGraceMin) BETWEEN #T.rStart AND #T.rEnd AND #ShLv.lateMins<>0 UPDATE #ShLv SET #ShLv.BalMins=tblEmpShortLeaveD.totMins-tblEmpShortLeaveD.UsedMins,#ShLv.balQty=tblEmpShortLeaveD.totQty-tblEmpShortLeaveD.UsedQty FROM tblEmpShortLeaveD,#ShLv WHERE #ShLv.RegID=tblEmpShortLeaveD.EmpID AND tblEmpShortLeaveD.cYear=@Year AND tblEmpShortLeaveD.cMonth=@Month SELECT * FROM #ShLv ORDER BY RegID END " : FK_EQ(sSQL, "S", "", False, False, False)
