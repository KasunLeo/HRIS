        sSQL = "CREATE PROCEDURE [dbo].[SP_NightConfirmSingle] (@atDate DATETIME,@EmpID NVARCHAR (6))  AS BEGIN CREATE TABLE #AtManager (RegID nvarchar (6),empNo nvarchar (6),dispName nvarchar (256),atDate datetime,intime1 datetime,outdate  datetime, outtime1 datetime,enrolNo numeric (18,0),Confirm bit,shift NVARCHAR (5),DayType NVARCHAR (5),ShiftMode Numeric (18,0))  INSERT INTO #AtManager  select tblGetINOut.EmpID,tblEmployee.empNo, tblEmployee.DIspName,tblGetINOut.atDate,tblGetINOut.inTime, tblGetINOut.atDate,tblGetINOut.outTime,tblEmployee.enrolNo,0,tblGetINOut.shiftID,tblGetINOut.dayTypeID,tblSetShiftH.ShiftMode from  tblGetINOut,tblEmployee,tblSetShiftH WHERE  tblEmployee.RegID=tblGetINOut.EmpID AND tblGetINOut.shiftID = tblSetShiftH.ShiftID AND tblGetINOut.atDate=@atDate and  tblGetINOut.outupdate=0 and tblGetINOut.antStatus=1 AND tblEmployee.regID=@EmpID order by tblGetINOut.outupdate ; CREATE TABLE #EmpRegister (regID nvarchar (6),atDate datetime,allShifts nvarchar (3),shiftID nvarchar (3),ShiftMode numeric (18,0)); INSERT INTO #EmpRegister  SELECT tblEmpRegister.empID,tblEmpRegister.atDate,tblEmpRegister.allshifts,tblEmpRegister.shiftID,tblSetShiftH.ShiftMode FROM tblEmpRegister,tblSetShiftH WHERE tblEmpRegister.allShifts=tblSetShiftH.shiftID AND  tblEmpRegister.atDate=@atDate  UPDATE #AtManager SET #AtManager.shift=#EmpRegister.allShifts,#AtManager.ShiftMode=#EmpRegister.ShiftMode FROM #EmpRegister,#AtManager where #AtManager.regID=#EmpRegister.regID AND #AtManager.atDate=#EmpRegister.atDate AND #AtManager.shift=#EmpRegister.shiftID; select tblDiMachine.EmpID,atDate,Min(tblDiMachine.tTime) AS 'Ttime',tblDiMachine.cDate as 'Cdate'   Into #T_Time From #AtManager,tblDiMachine WHERE #AtManager.EnrolNo = tblDiMachine.EmpID  AND  tblDiMachine.cDate = DateAdd(Day,1,#AtManager.atDate) AND tblDiMachine.Capture In (0,1,2,3) GROUP BY tblDiMachine.EmpID , tblDiMachine.cDate ,#AtManager.atDate ;  UPDATE #AtManager SET #AtManager.outtime1 = #T_Time.tTime , #AtManager.outDate = #T_Time.cDate,#AtManager.confirm=1  FROM #T_Time,#AtManager WHERE #T_Time.EmpID = #AtManager.EnrolNo AND #T_Time.atDate = #AtManager.AtDate;  UPDATE #AtManager SET #AtManager.dayType=tblDayType.shortCode FROM #AtManager,tblDayType WHERE #AtManager.dayType = tblDayType.typeID UPDATE #AtManager SET #AtManager.shift=tblSetShiftH.shortCode FROM #AtManager,tblSetShiftH WHERE #AtManager.shift = tblSetShiftH.shiftID  select RegID,EmpNo,DispName,CONVERT(VARCHAR(10),atDate,111) AS 'InDate', CASE WHEN CONVERT(VARCHAR(8),inTime1,108)='00:00:00' THEN '' ELSE CONVERT(VARCHAR(8),inTime1,108) END AS 'InTime',CONVERT(VARCHAR(10),outDate,111) AS 'OutDate',CASE WHEN CONVERT(VARCHAR(8),outTime1,108)='00:00:00' THEN '' ELSE CONVERT(VARCHAR(8),outTime1,108) END AS 'OutTime', CASE WHEN datediff(hh,InTime1,outTime1)<0 THEN 0 ELSE datediff(hh,InTime1,outTime1) END AS workHrs, shift AS shiftName,dayType AS 'dayType',0 AS normalOT,0 AS doubleOT,0 AS tripleOT,0 AS totalOT,CONVERT(BIT,CASE WHEN datediff(hh,InTime1,outTime1)<=25 and confirm =1 THEN 1  WHEN datediff(hh,InTime1,outTime1)>25 AND Confirm =1 then 0 ELSE 0 END) AS Confirm,shiftMode  from #AtManager END" : FK_EQ(sSQL, "S", "", False, False, False)

