        sSQL = "CREATE PROCEDURE SP_MovementDetailReport (@ST  DATETIME, @ED  DATETIME) AS BEGIN  DELETE FROM tblMovement INSERT INTO tblMovement SELECT tblEmployee.RegID,tblEmployee.EnrolNo,tblEmployee.dispName,tblEmpregister.AtDate,'','','' from tblEmployee,tblEmpRegister WHERE tblEmployee.RegID = tblEmpRegister.EmpID AND tblEmpRegister.AtDate  Between @ST AND @ED order by regid ;CREATE TABLE #T (EnrolNo Numeric (18,0),cDate DateTime,ManualFinger Nvarchar (200),RemovedFinger Nvarchar (200) ,origFinger Nvarchar (200)) ;INSERT INTO #T SELECT    SS.EmpID,SS.cDate as 'Kdate','' as 'ManualFinger', '' as 'RemovedFinger',   STUFF((SELECT '; ' + Convert(Nvarchar(5),US.cTime,108) FROM tblDiMachine US WHERE US.EmpID = SS.EmpID AND US.cDate = SS.cDate ORDER BY cTime  FOR XML PATH('')), 1, 1, '') [RESULT] FROM tblDiMachine SS WHERE SS.cDate Between @ST AND @ED GROUP BY SS.EmpID,SS.cDate, SS.EmpID,SS.cDate ORDER BY SS.EmpID,SS.cDate ;CREATE TABLE #T1 (EnrolNo Numeric (18,0),cDate DateTime,ManualFinger Nvarchar (200)) ;INSERT INTO #T1 SELECT    SS.EmpID,SS.cDate as 'Kdate',   STUFF((SELECT '; ' + Convert(Nvarchar(5),US.cTime,108) FROM tblDiMachine US WHERE US.EmpID = SS.EmpID AND US.cDate = SS.cDate AND SS.editMode=US.editMode ORDER BY cTime  FOR XML PATH('')), 1, 1, '') [RESULT] FROM tblDiMachine SS WHERE SS.cDate Between @ST AND @ED AND SS.editMode=1 GROUP BY SS.EmpID,SS.cDate,SS.editMode, SS.EmpID,SS.cDate,SS.editMode ORDER BY SS.EmpID,SS.cDate ;CREATE TABLE #T2 (EnrolNo Numeric (18,0),cDate DateTime,RemovedFinger Nvarchar (200)) ;INSERT INTO #T2 SELECT    SS.EmpID,SS.cDate as 'Kdate',   STUFF((SELECT '; ' + Convert(Nvarchar(5),US.cTime,108) FROM tblDiMachine US WHERE US.EmpID = SS.EmpID AND US.cDate = SS.cDate AND SS.capture=US.capture ORDER BY cTime  FOR XML PATH('')), 1, 1, '') [RESULT] FROM tblDiMachine SS WHERE SS.cDate Between @ST AND @ED AND SS.capture=9  GROUP BY SS.EmpID,SS.cDate,SS.capture, SS.EmpID,SS.cDate,SS.capture ORDER BY SS.EmpID,SS.cDate ;UPDATE #T SET #T.ManualFinger = #T1.ManualFinger FROM #T,#T1 WHERE #T.EnrolNo = #T1.EnrolNo AND #T.cDate  = #T1.cDate ;UPDATE #T SET #T.RemovedFinger = #T2.RemovedFinger FROM #T,#T2 WHERE #T.EnrolNo = #T2.EnrolNo AND #T.cDate  = #T2.cDate ;UPDATE tblMovement SET tblMovement.origFinger = #T.origFinger, tblMovement.ManualFinger = #T.ManualFinger, tblMovement.RemovedFinger = #T.RemovedFinger FROM #T,tblMovement WHERE #T.EnrolNo = tblMovement.EnrolNo AND #T.cDate  = tblMovement.AtDate ;DROP TABLE #T ;DROP TABLE #T1; DROP TABLE #T2 ;delete from [tblMovement]  where manualfinger='' and  removedfinger='' END " : FK_EQ(sSQL, "S", "", False, False, False)
