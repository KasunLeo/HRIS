        sSQL = "CREATE PROCEDURE [dbo].[SP_GenShortLeave] (@cYear INTEGER,@month INTEGER,@maxMin AS INTEGER,@MaxQty INTEGER) AS BEGIN BEGIN CREATE TABLE #T (id NUMERIC (18,0),regID NVARCHAR (6)) INSERT INTO #T select rank() OVER (ORDER BY regID) as rank,regID from tblEmployee WHERE empStatus<>9 AND regID NOT IN (SELECT DISTINCT EmpID FROM [tblEmpShortLeaveD] WHERE cYear=@cYear) order by rank DECLARE @totEmp AS INTEGER DECLARE @int AS INTEGER DECLARE @mCount AS INTEGER SET @int=1 SET @totEmp = (SELECT count(regID) FROM #T) WHILE (@int <=@totEmp) BEGIN INSERT INTO tblEmpShortLeaveD SELECT regID,@cYear,@month,@maxMin,0,0,@maxMin,@MaxQty,0,@MaxQty FROM #T WHERE id=@int; SET @mCount=2 WHILE (@mCount<=12) BEGIN INSERT INTO tblEmpShortLeaveD SELECT regID,@cYear,@mCount,@maxMin,0,0,@maxMin,@MaxQty,0,@MaxQty FROM #T WHERE id=@int; SET @mCount=@mCount+1 END SET @int=@int+1 END END END " : FK_EQ(sSQL, "S", "", False, False, False)
