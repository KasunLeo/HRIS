        sSQL = "CREATE PROCEDURE [sp_ProcessRosterPatn] (@dateFrom datetime,@dateTo datetime, @gap Numeric (18,0),@EmpID Nvarchar (6),@PtnID Nvarchar(3)) AS BEGIN DECLARE @dates TABLE(row_id numeric (18,0),stDate DateTime , eddate datetime) CREATE TABLE #V (row_id numeric (18,0),stDate DateTime , eddate datetime)    Declare @st DateTime Declare @ed DateTime SET @st = @DateFrom SET @ed = @DateTo Declare @V Numeric (18,0) Declare @StDate DateTime Declare @edDate DateTime Declare @Val Numeric (18,0) SET @Val = 1 WHILE(@dateFrom <= @dateTo) BEGIN     SET @StDate = @dateFrom     SELECT @dateFrom = DATEADD(day, @gap,@dateFrom)   SET @V = DateDiff(Day,@datefrom,@dateto)     if @V <0     begin     set @datefrom = @DateTo     end     INSERT INTO @dates     SELECT @val,@stDate,@dateFrom     SET @Val = @Val + 1 SET @dateFrom = DateAdd(Day,1,@dateFrom) END INSERT INTO #V SELECT * FROM @dates CREATE TABLE #T (PtnID Nvarchar (3),EmpID Nvarchar (6),w_Date DateTime,DaytypeID Nvarchar (3),ShiftID Nvarchar(3), C_Sector Numeric (18,0),LineNumber Numeric (18,0)) INSERT INTO #T SELECT @PtnID,EmpID,AtDate,DayTypeID,AllShifts,0,0 FROM tblEmpRegister WHERE AtDate Between @st AND @ed AND EmpID = @EmpID Order by AtDate UPDATE #T SET #T.C_Sector = #V.Row_ID FROM #T,#V WHERE #T.w_Date Between #V.stDate AND #V.edDate SELECT ptnID,EmpID,w_Date,DayTypeID,ShiftID,C_Sector,ROW_NUMBER() OVER(PARTITION BY [C_Sector] ORDER BY [C_Sector]) as s_index INTO #P FROM #T DELETE FROM #T INSERT INTO #T SELECT * FROM #P UPDATE #T SET #T.DayTypeID = tblRosterPatern.DayTypeID,#T.ShiftID = tblRosterPatern.ShiftID FROM #T,tblRosterPatern WHERE #T.PtnID = tblRosterPatern.P_ID AND #T.LineNumber = tblRosterPatern.Seq_ID UPDATE tblEmpRegister SET tblEmpRegister.DayTypeID = #T.DayTypeID , tblEmpRegister.AllShifts = #T.ShiftID FROM #T,tblEmpRegister WHERE #T.EmpID = tblEmpRegister.EmpID AND #T.w_date = tblEmpRegister.AtDate END  " : FK_EQ(sSQL, "P", "", False, False, False)
