nt_rpt_month_summery.JoinRelDays = (SELECT COUNT(#T.JoinRelDays) FROM #T  WHERE #T.EmpNo= Absent_rpt_month_summery.EmpNo AND #T.JoinRelDays = 1) UPDATE  Absent_rpt_month_summery SET Absent_rpt_month_summery.ShortLeaves  = (SELECT SUM(#T.ShortLeaves) FROM #T  WHERE #T.EmpNo= Absent_rpt_month_summery.EmpNo) UPDATE Absent_rpt_month_summery SET Absent_rpt_month_summery.SickLeaves  = (SELECT SUM(#T.SickLeaves) FROM #T  WHERE #T.EmpNo= Absent_rpt_month_summery.EmpNo) UPDATE Absent_rpt_month_summery SET Absent_rpt_month_summery.NoPayLeaves  = (SELECT SUM(#T.NoPayLeaves) FROM #T WHERE #T.EmpNo= Absent_rpt_month_summery.EmpNo) UPDATE Absent_rpt_month_summery SET Absent_rpt_month_summery.anualLv  = (SELECT SUM(#T.anlLeave) FROM #T WHERE #T.EmpNo= Absent_rpt_month_summery.EmpNo) UPDATE Absent_rpt_month_summery SET Absent_rpt_month_summery.medicalLv  = (SELECT SUM(#T.medLeave) FROM #T WHERE #T.EmpNo= Absent_rpt_month_summery.EmpNo) UPDATE Absent_rpt_month_summery SET Absent_rpt_month_summery.NoPay = CASE WHEN Absent_rpt_month_summery.NoPay < 0 THEN 0 ELSE Absent_rpt_month_summery.NoPay  END UPDATE Absent_rpt_month_summery SET Absent_rpt_month_summery.actualNoPay = CASE WHEN Absent_rpt_month_summery.actualNoPay < 0 THEN 0 ELSE Absent_rpt_month_summery.actualNoPay  END UPDATE Absent_rpt_month_summery SET Absent_rpt_month_summery.actualNoPay = CASE WHEN Absent_rpt_month_summery.actualNoPay <> 1000 THEN Absent_rpt_month_summery.actualNoPay + Absent_rpt_month_summery.NoPayLeaves ELSE Absent_rpt_month_summery.actualNoPay  END END" : FK_EQ(sSQL, "S", "", False, False, False)
